        <file contents="using System;&#xA;using br.ufc.pargo.hpe.backend.DGAC;&#xA;using br.ufc.pargo.hpe.basic;&#xA;using br.ufc.pargo.hpe.kinds;&#xA;using lu.problem_size.Instance_LU;&#xA;using common.problem_size.Class;&#xA;using lu.Exchange4;&#xA;&#xA;namespace impl.lu.Exchange4Impl { &#xA;&#x9;public class IExchange4Impl&lt;I, C> : BaseIExchange4Impl&lt;I, C>, IExchange4&lt;I, C>&#xA;&#x9;where I:IInstance_LU&lt;C>&#xA;&#x9;where C:IClass {&#xA;&#x9;   &#xA;&#x9;    private double[,] g;&#xA;&#x9;    private double[,] h;&#xA;&#x9;    private int ibeg;&#xA;&#x9;    private int ifin1;&#xA;&#x9;    private int jbeg; &#xA;&#x9;    private int jfin1;&#xA;&#x9;    &#xA;&#x9;&#x9;public IExchange4Impl() { &#xA;&#x9;&#x9;&#xA;&#x9;&#x9;} &#x9;&#x9;&#xA;&#x9;&#x9;public override void compute() { &#xA;            int i, j, ny2;&#xA;            int from_s = 1, from_e = 3;&#xA;            ny2 = ny + 2;&#xA;            if(jfin1==ny) {&#xA;                double[] dum;&#xA;                dum = Input_buffer.Array = new double[2*nx];&#xA;                ShiftToWest.initiate_recv();//msgid3 = worldcomm.ImmediateReceive&lt;double>(east, from_e, dum);msgid3.Wait();&#xA;                for(i = 1; i&lt;=nx; i++) {&#xA;                    g[ny+1, i] = dum[i-1];&#xA;                    h[ny+1, i] = dum[i+nx-1];&#xA;                }&#xA;            }&#xA;            if(jbeg==1) {&#xA;                double[] dum;&#xA;                dum = Output_buffer.Array = new double[2*nx];&#xA;                for(i = 1; i&lt;=nx; i++) {&#xA;                    dum[i-1]    = g[1, i];&#xA;                    dum[i+nx-1] = h[1, i];&#xA;                }&#xA;                ShiftToWest.initiate_send();//worldcomm.Send&lt;double>(dum, west, from_e);&#xA;            }&#xA;            if(ifin1==nx) {&#xA;                double[] dum;&#xA;                dum = Input_buffer.Array = new double[2*ny2];&#xA;                ShiftToNorth.initiate_recv();//msgid1 = worldcomm.ImmediateReceive&lt;double>(south, from_s, dum); msgid1.Wait();&#xA;                for(j = 0; j&lt;=ny+1; j++) {&#xA;                    g[j, nx+1] = dum[j];&#xA;                    h[j, nx+1] = dum[j+ny2];&#xA;                }&#xA;            }&#xA;            if(ibeg==1) {&#xA;                double[] dum;&#xA;                dum = Output_buffer.Array = new double[2*ny2];&#xA;                for(j = 0; j&lt;=ny+1; j++) {&#xA;                    dum[j]     = g[j, 1];&#xA;                    dum[j+ny2] = h[j, 1];&#xA;                }&#xA;                ShiftToNorth.initiate_send();//worldcomm.Send&lt;double>(dum, north, from_s);&#xA;            }&#xA;&#x9;&#x9;}&#xA;&#x9;&#x9;public void setParameters(double[,] g, double[,] h, int ibeg, int ifin1, int jbeg, int jfin1){&#xA;&#x9;&#x9;   this.g     = g;&#xA;&#x9;&#x9;   this.h     = h;&#xA;&#x9;&#x9;   this.ibeg  = ibeg;&#xA;&#x9;&#x9;   this.ifin1 = ifin1;&#xA;&#x9;&#x9;   this.jbeg  = jbeg;&#xA;&#x9;&#x9;   this.jfin1 = jfin1;&#xA;&#x9;&#x9;   &#xD;&#xA;&#x9;&#x9;}&#xA;&#x9;}&#xA;}&#xA;" fileType="library" name="IExchange4Impl.cs" srcType="user" uri="impl.lu.Exchange4Impl/bin/1.0.0.0/IExchange4Impl.dll" versionId="1.0.0.0">
