using System;
using DGAC;
using hpe.basic;
using hpe.kinds;
using data.Function;
using data.IntegralCase;

namespace data.impl.IntegralCase
{

    public class IIntegralCaseImpl<F> : BaseIIntegralCaseImpl<F>, IIntegralCase<F>
    where F : IFunction
    {

        double a { set { xinf.Value = value; } get { return xinf.Value; } }
        double b { set { xsup.Value = value; } get { return xsup.Value; } }

        double f(double x)
        {
            return function(x);
        }

        public IIntegralCaseImpl()
        {

        }


    }

}
