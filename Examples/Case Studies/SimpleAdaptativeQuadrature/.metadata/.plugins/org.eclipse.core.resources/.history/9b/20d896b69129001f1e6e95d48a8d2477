using System;
using DGAC;
using hpe.basic;
using hpe.kinds;
using environment.MPIDirect;
using MPI;

namespace environment.impl.MPIDirectImpl { 

public class IMPIDirectImpl: hpe.kinds.Environment, IMPIDirect
{

private MPI.Environment mpi = null;

public MPI.Environment MPI {
   get { return mpi; }
}


public IMPIDirectImpl() { 

    string[] args = System.Environment.GetCommandLineArgs();
   	mpi = new MPI.Environment(ref args);

} 

public void Dispose()
{
    mpi.Dispose();
}


public void listen() {}
public void stopListen() {}

public static string UID = "";

override public void createSlices() {
	base.createSlices();
} 


}

}
