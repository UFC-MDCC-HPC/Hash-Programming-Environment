using System;
using DGAC;
using hpe.basic;
using hpe.kinds;
using data.Function;
using jefferson.data.Double;
using data.IntegralArea;
using environment.MPIDirect;
using skeleton.Farm.Distribute.AQPartitionInterval;

namespace skeleton.impl.Farm.Distribute.AQPartitionIntervalImpl { 

public class IPartitionIntervalDistributeImpl<Ds>: Synchronizer, IPartionIntervalDistribute<Ds>
where Ds:IIntegralArea
{

private IFunction function = null;

public IFunction Function {
	set {
		this.function = value;
	}
}

private IDouble xsup = null;

public IDouble Xsup {
	set {
		this.xsup = value;
	}
}

private IDouble xinf = null;

public IDouble Xinf {
	set {
		this.xinf = value;
	}
}

private Ds data = default(Ds);

public Ds Data {
	set {
		this.data = value;
	}
}

private IMPIDirect mpi = null;

public IMPIDirect Mpi {
	set {
		this.mpi = value;
	}
}


public IPartitionIntervalDistributeImpl() { 

} 

public static string UID = "";

override public void createSlices() {
	base.createSlices();
} 

public void synchronize() { 

	;

} // end activate method 

}

}
