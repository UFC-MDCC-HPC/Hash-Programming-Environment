using hpe.kinds;
using jefferson.data.Data;

namespace data.Trapezium { 

public interface ITrapezium : IData, IDataStructureKind 
{
     int XLeft     { get; set; }
     int XRight    { get; set; }
     int YBottom   { get; set; }
     int YTopRight { get; set; }
     int YTopLeft  { get; set; }

} // end main interface 

} // end namespace 
